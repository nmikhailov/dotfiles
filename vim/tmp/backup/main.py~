#/usr/bin/env python


def lvl0():
    s = 'WYV I HIS F AOL LHZPLZAJP WOL YWVZZPISLPZ AOL ZOPMAJP WOL YHSZVRUVDUHZJHLZHYJP WOL YPUAOPZJ HZL AOL WHZZDVYKPZDVYKBU WYV MPA HIS LDYPAALUPUSVDLYJ HZL'
