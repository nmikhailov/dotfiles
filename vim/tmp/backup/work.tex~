\begin{table}[h]
    \centering
    \caption{Экспериментальные результаты, слабые требования к PST (3-8)}
    \begin{tabular}{l l l l p{1cm}l}
        % Header
        \hline
        \multirow{2}{*}{Метод} &
        \multirow{2}{*}{Заблокированных} &
        \multicolumn{2}{c}{PST} &
        \multirow{2}{1cm}{Средняя стоимость}\\

        \cline{3-4}
        &&Среднее&Удовол.&&\\
        \hline

        % Body
        Shared path  &  0.638  &  9.18   &  23.3\%  &  0.541  \\ \hline
        Shared link  &  0.438  &  9.149  &  16.9\%  &  0.450  \\ \hline
        Proposed     &  0.751  &  6.254  &  100\%   &  0.268  \\ \hline
    \end{tabular}
    \label{tab:table1}
\end{table}
\begin{table}[h]
    \centering
    \caption{Экспериментальные результаты, сильные требования к PST (15-20)}
    \begin{tabular}{l l l l p{1cm}l}
        % Header
        \hline
        \multirow{2}{*}{Метод} &
        \multirow{2}{*}{Заблокированных} &
        \multicolumn{2}{c}{PST} &
        \multirow{2}{1cm}{Средняя стоимость}\\

        \cline{3-4}
        &&Среднее&Удовол.&&\\
        \hline

        % Body
        Shared path  &  0.612  &  9.124  &  98.4\%  &  0.481  \\ \hline
        Shared link  &  0.696  &  9.079  &  99.0\%  &  0.510  \\ \hline
        Proposed     &  0.579  &  9.052  &  100\%   &  0.617  \\ \hline
    \end{tabular}
    \label{tab:table2}
\end{table}
